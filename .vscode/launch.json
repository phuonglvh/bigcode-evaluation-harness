    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current Module",
            "type": "python",
            "request": "launch",
            "module": "${relativeFileDirname}.${fileBasenameNoExtension}"
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${relativeFile}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: generations_to_codexglue_bleu",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/utils/generations_to_codexglue_bleu.py",
            "args": [
                "-gen_path",
                "generations_py.json",
                "-pred_path",
                "results/bleu/predictions_py.txt"
            ],
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Python: generations_to_codexglue_codebleu",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/utils/generations_to_codexglue_codebleu.py",
            "args": [
                "-gen_path",
                "generations_py.json",
                "-pred_path",
                "results/codebleu/predictions_py.txt"
            ],
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Python: human_eval_x_to_codexglue_bleu",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/utils/human_eval_x_to_codexglue_bleu.py",
            "args": [
                "-lang",
                "python",
                "-ref_path",
                "datasets/references/bleu/references_2_py.jsonl",
                "-num_prob",
                "164"
            ],
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Python: human_eval_x_to_codexglue_codebleu",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/utils/human_eval_x_to_codexglue_codebleu.py",
            "args": [
                "-lang",
                "python",
                "-ref_path",
                "datasets/references/codebleu/references_2_py.txt",
                "-num_prob",
                "164"
            ],
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Python: main",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "args": [
                "--model",
                "codeparrot/codeparrot-small",
                "--tasks",
                // "humaneval",
                // "multiple-js",
                // "multiple-java",
                "multiple-py",
                // "ds1000-numpy-completion",
                // "concode",
                "--max_length_generation",
                "512",
                "--temperature",
                "0.8",
                "--top_p",
                "0.95",
                "--top_k",
                "0",
                "--n_samples",
                "1",
                "--batch_size",
                "1",
                "--allow_code_execution",
                "--limit",
                "2",
                "--generation_only",
                "--save_generations",
                "--save_generations_path",
                "generations_java.json",
                "--trust_remote_code",
                "--save_references"
            ],
            "env": {
                "PYTORCH_ENABLE_MPS_FALLBACK": "1"
            },
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "python: eval multiple-java",
            "type": "python",
            "request": "launch",
            "program": "main.py",
            "args": [
                "--model",
                "codellama/CodeLlama-13b-Python-hf",
                "--tasks",
                "multiple-java",
                "--max_length_generation",
                "1024",
                "--temperature",
                "0.8",
                "--top_p",
                "0.95",
                "--top_k",
                "0",
                "--seed",
                "0",
                "--n_samples",
                "200",
                "--batch_size",
                "10",
                "--precision",
                "bf16",
                "--allow_code_execution",
                "--trust_remote_code",
                "--limit_start",
                "0",
                "--limit",
                "2",
                "--save_every_k_tasks",
                "20",
                "--load_generations_path",
                "./benchmark/codellama-13b-python/java/CodeLlama-13b-Python-hf-temp0.8-p0.95-bf16-n200-batch10-maxlen1024-java-generations_multiple-java.json",
                "--metric_output_path",
                "./benchmark/codellama-13b-python/java/improve/t0.8-p0.95-k0/CodeLlama-13b-Python-hf-temp0.8-p0.95-k0-bf16-n200-batch10-maxlen1024-java-generations-0-50_multiple-java-eval-0-2-evaluation_results.json",
                "--save_references"
            ],
            "justMyCode": false,
            "console": "integratedTerminal"
        },
        {
            "name": "code2code: py-java",
            "request": "launch",
            "type": "python",
            "program": "code_to_code_trans.py",
            "justMyCode": false,
            "console": "integratedTerminal",
            "env": {
                "PYTORCH_ENABLE_MPS_FALLBACK": "1"
            },
            "args": [
                "--model",
                "codeparrot/codeparrot-small",
                "--tasks",
                "code2code-multiple-java",
                "--max_length_generation",
                "1024",
                "--temperature",
                "0.2",
                "--top_p",
                "0.95",
                "--top_k",
                "0",
                "--seed",
                "0",
                "--n_samples",
                "1",
                "--batch_size",
                "1",
                "--precision",
                "bf16",
                "--allow_code_execution",
                "--trust_remote_code",
                "--save_every_k_tasks",
                "1",
                "--save_generations",
                "--save_generations_path",
                "${workspaceFolder}/benchmark/codeparrot-small/code2code/py-java/codeparrot-small-temp0.2-p0.95-bf16-n1-batch1-maxlen1024-java-translations-0-1.json",
                "--limit_start",
                "0",
                "--limit",
                "1",
                "--save_references",
                "--save_references_path",
                "${workspaceFolder}/benchmark/codeparrot-small/code2code/py-java/codeparrot-small-temp0.2-p0.95-bf16-n1-batch1-maxlen1024-java-references-0-1.json",
                "--source_generations_path",
                "${workspaceFolder}/benchmark/CodeLlama-13b-Python-hf/py/improve/pass@1/t0.2-p0.95-k0-batch1-n1/CodeLlama-13b-Python-hf-temp0.2-p0.95-k0-bf16-n1-seed5-batch1-maxlen1024-py-generations-0-158_multiple-py.json",
                "--source_lang",
                "py",
            ]
        }
    ]
}